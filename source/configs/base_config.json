{
  "dataset": "cifar",
  "dataroot": "./input",
  "anopath": "./input",
  "workers": 4,
  "ngpu": 1,
  "debug": false,
  "logdir": "results/DCGAN_noema",
  "mode": "train",

  "netG": "DCGAN",
  "netD": "DCGAN",
  "conditional": false,
  "nz": 128,
  "ngf": 64,
  "ndf": 64,
  "nc": 3,
  "G_conv": "convT",
  "G_normalization": "BN",
  "G_activation": "relu",
  "G_linear": "linear",
  "G_bias": false,
  "D_conv": "conv",
  "D_normalization": "BN",
  "D_activation": "lrelu",
  "D_linear": "linear",
  "D_bias": false,
  "projection_dim": 128,
  "conditioning": "concat",
  "regularization": "",
  "gp_lamb": 10,
  "ortho_strength": 0.0001,

  "disc_iters": 1,
  "loss": "ns",

  "batchSize": 64,
  "imageSize": 32,
  "num_classes":10,
  "nepochs": 300,
  "lrG": 0.0002,
  "lrD": 0.0002,
  "beta1D": 0.5,
  "beta1G": 0.5,
  "beta2D": 0.999,
  "beta2G": 0.999,
  "lrdecay": false,

  "use_ema": false,
  "restore": "",
  "save_every": 20000,
  "log_every": 100,
  "test_every": 1000,
  "test_inception_every": 15000,
  "num_inception_imgs": 50000,
  "inception_cache_path": "inception_cache/cifar",

  "anomaly_frac": 0.05,
